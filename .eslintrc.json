{
    "env": {
        "node": true,
        "es6": true,
        "commonjs": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": ["error"]
    }
}
